<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFAC1919</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textAllCaps">false</item>
<item name="fontFamily">@font/kokn</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Department" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <!-- Customize your theme here. -->

        <item name="colorPrimaryDark">#000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>

        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>

    </style>

    <style name="dialog_light" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@drawable/rounded_corner_spinner</item>
        <item name="android:textColor">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    style="@style/Widget.AppCompat.RatingBar.Indicator"

    <style name="StarRatingBars" parent="@android:style/Widget.RatingBar">
        <item name="android:indeterminateDrawable">@color/duskYellow</item>
        <item name="android:progressDrawable">@color/lightGrey</item>
    </style>

    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="colorControlNormal">@color/gray</item>
        <item name="colorControlActivated">@color/duskYellow</item>
    </style>


    <style name="MyRatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="colorAccent">@color/duskYellow</item>
        <item name="android:colorControlNormal">#f9a825</item>
    </style>
    <style name="Divider">
        <item name="android:background">?android:attr/listDivider</item> //you can give your color here. that will change all divider color in your app.
    </style>

    <style name="Divider.Horizontal" parent="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item> // You can change thickness here.

    </style>

    <style name="Divider.Vertical" parent="Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>


</resources>
